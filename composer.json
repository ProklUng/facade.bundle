{
    "name": "proklung/facade-bundle",
    "description": "Laravel like facades for Symfony.",
    "type": "symfony-bundle",
    "license": "MIT",
    "authors": [
        {
            "name": "Gavrilov Fedy",
            "email": "fedor.gavrilov.75@mail.ru",
            "homepage": "https://github.com/ProklUng"
        }
    ],
    "support": {
        "issues": "https://github.com/ProklUng/facade.bundle/issues",
        "source": "https://github.com/ProklUng/facade.bundle"
    },
    "autoload": {
        "psr-4": {
            "Prokl\\FacadeBundle\\": ""
        },
        "exclude-from-classmap": [
            "Tests/"
        ]
    },
    "require": {
        "php": ">=7.3",
        "psr/container": "^1.0",
        "symfony/dependency-injection": "^3.4 || ^4.0 || ^5.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^7.5.2 || ^8.0 || ^9.0",
        "symfony/framework-bundle": "^3.4 || ^4.0 || ^5.0",
        "mockery/mockery": "^1.4"
    },
    "config": {
        "preferred-install": {
            "*": "dist"
        },
        "sort-packages": true
    }
}
